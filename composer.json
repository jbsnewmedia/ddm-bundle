{
    "name": "jbsnewmedia/ddm-bundle",
    "description": "Data Definition Model Bundle for VIS",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Juergen Schwind",
            "email": "juergen.schwind@jbs-newmedia.de"
        }
    ],
    "require": {
        "php": ">=8.3",
        "symfony/framework-bundle": "7.4.*",
        "symfony/translation-contracts": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "JBSNewMedia\\DDMBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JBSNewMedia\\DDMBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    }
}
