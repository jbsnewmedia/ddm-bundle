{% extends '@Vis/base.html.twig' %}

{% block javascripts %}
    {% if assets is defined %}
        {% for asset in assets %}
            {% do addAssetComposer(asset) %}
        {% endfor %}
    {% else %}
        {% do addAssetComposer('avalynx/avalynx-form/src/js/avalynx-form.js') %}
    {% endif %}

    {{ parent() }}
{% endblock %}

{% block body %}
    {% block form_content %}{% endblock %}
{% endblock %}
